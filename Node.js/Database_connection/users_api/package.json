{
  "name": "API",
  "version": "1.0.0",
  "description": "Http methods",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start:dev": "nodemon --watch index.js --watch src/ --exec babel-node index.js",
    "start": "node dist/index.js",
    "build": "rimraf dist && babel src  --copy-files --out-dir dist",
    "migrate": "yarn knex migrate:latest --knexfile src/knexfile.js",
    "rollback": "yarn knex migrate:rollback --knexfile src/knexfile.js",
    "prettify": "yarn prettier --loglevel silent --write **/*.{js,json,md}",
    "lint": "eslint src index.js --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "camelize": "^1.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.2",
    "joi": "^17.2.1",
    "knex": "^0.21.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "snakeize": "^0.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "eslint": "^7.8.1",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2"
  }
}
